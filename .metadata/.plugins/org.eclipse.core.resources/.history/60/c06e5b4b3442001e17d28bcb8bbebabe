package day07;

public class Method06 {

	public static void main(String[] args) {
             /* 구구단 출력
              * 단 1개를 주고, 구구단 출력
              * */
		//2~9까지 반복하여 모든 단을 출력
	a();	
	}
	/*기능 : 단이 주어지면 그 단의 구구단을 출력(리턴x)
	 * */

	public static void a() {
		for(int i=2; i<=9; i++) {
			for(int j=1; j<=9; j++) {
				System.out.println(i*j);
			}
		}
	}
}
