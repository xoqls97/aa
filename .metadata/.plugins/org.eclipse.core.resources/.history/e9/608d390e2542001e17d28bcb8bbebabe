package day07;

import java.util.Scanner;

public class Method05 {

	public static void main(String[] args) {
		/* 2~100까지의 소수를 출력
		 * 2~100까지 for문을 돌려서 메서드 호출하여 true라면 출력 
		 * */

		a();
	}
	//기능: 정수가 주어지면 이 정수가 소수인지 아닌지 판단(true/false)
	public static void a() {
		for(int i=2; i<101; i++) {
			for(int j=1; j<=i; j++) {
				if(i%j==0) {
				}
				System.out.println(i);
			}
		}
		



}
}
