package day10;

import java.util.Scanner;

public class ClassEx02 {

	public static void main(String[] args) {

		/* 자동차 클래스 생성
		 * 멤버변수 :
		 * 메서드 : 전원,스피드up, 스피드down

		 * 
		 */
		Car car = new Car();
		car.power();
		car.power();
		
		
	}

}
class Car{
	
private	int speed;
private	boolean power;

public void power() {
	if(!power) {
		power =true;
		System.out.println("시동이켜졋음");
	}else {
		power = false;
		System.out.println("시동이꺼짐");
	}
}
	


	
	
	



	
}
