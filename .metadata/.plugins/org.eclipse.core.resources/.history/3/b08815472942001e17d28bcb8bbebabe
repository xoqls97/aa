package day07;


public class Method05 {

	public static void main(String[] args) {
		/* 2~100까지의 소수를 출력
		 * 2~100까지 for문을 돌려서 메서드 호출하여 true라면 출력 
		 * */
		a();			
		
	}
	//기능: 정수가 주어지면 이 정수가 소수인지 아닌지 판단(true/false)
	public static void a() {
		for(int i=2; i<=100; i++) {
			for(int j=2; j<=100; j++) {
				if(i!=j && i%j == 0) {
					break;
				}
				if(i == j) {
					System.out.println("true");
					
				}else {
					System.out.println("false");
				}
		
			
			
				}
			
		}
	}
		
	
}

 
	
		

